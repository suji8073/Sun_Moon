<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sun_moon">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Sun_Moon">
        <activity
            android:name=".loading"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.example.sun_moon.login" android:label="login"/>
        <activity android:name="com.example.sun_moon.list" android:label="list"/>

        <activity android:name="com.example.sun_moon.sun_loading" android:label="sun_loading"/>
        <activity android:name="com.example.sun_moon.clap_loading" android:label="clap_loading"/>

        <activity android:name="com.example.sun_moon.exercise_camera" android:label="exercise_camera"/>

        <activity android:name="com.example.sun_moon.clap_exercise_intro" android:label="exercise_intro"/>
        <activity android:name="com.example.sun_moon.sun_exercise_intro" android:label="sun_exercise_intro"/>

        <activity android:name="com.example.sun_moon.sun_rest" android:label="sun_rest"/>
        <activity android:name="com.example.sun_moon.clap_rest" android:label="clap_rest"/>

        <activity android:name="com.example.sun_moon.sun_exercise_screen" android:label="sun_exercise_screen"/>
        <activity android:name="com.example.sun_moon.sun_exercise_screen_1" android:label="sun_exercise_screen_1"/>

        <activity android:name="com.example.sun_moon.clap_exercise_screen" android:label="clap_exercise_screen"/>
        <activity android:name="com.example.sun_moon.clap_exercise_screen_1" android:label="clap_exercise_screen_1"/>

        <activity android:name="com.example.sun_moon.scoreboard" android:label="scoreboard"/>



    </application>

</manifest>